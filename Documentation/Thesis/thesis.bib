number = {6},
pages = {701--708},
title = {{A mechanized gait trainer for restoration of gait}},
url = {http://citeseerx.ist.psu.edu/viewdoc/download?doi=10.1.1.554.3093{\&}rep=rep1{\&}type=pdf},
volume = {37},
