booktitle = {Dynamic Systems and Control},
file = {::},
mendeley-groups = {Haptics},
number = {1},
title = {{The PHANToM Haptic Interface: A Device for Probing Virtual Objects}},
url = {https://alliance.seas.upenn.edu/{~}medesign/wiki/uploads/Courses/Massie94-DSC-Phantom.pdf},
volume = {55},
year = {1994}
}


