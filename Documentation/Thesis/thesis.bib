title = {{Serious Games for Stroke Rehabilitation Employing Immersive User Interfaces in 3D Virtual Environment}},
url = {http://www.jhi-sbis.saude.ws/ojs-jhi/index.php/jhi-sbis/article/view/370/210},
volume = {6},
